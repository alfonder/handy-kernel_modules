{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/lib/gcc/x86_64-linux-gnu/9/include",
                "${workspaceFolder}/../linux/arch/x86/include",
                "${workspaceFolder}/../linux/arch/x86/include/generated",
                "${workspaceFolder}/../linux/include",
                "${workspaceFolder}/../linux/arch/x86/include/uapi",
                "${workspaceFolder}/../linux/arch/x86/include/generated/uapi",
                "${workspaceFolder}/../linux/include/uapi",
                "${workspaceFolder}/../linux/include/generated/uapi",
                "${workspaceFolder}/../linux/include/linux"
            ],
            "defines": [
                "__GNUC__",
                "__KERNEL__",
                "MODULE"
            ],
            "cStandard": "c99",
            "intelliSenseMode": "clang-x64",
            "compileCommands": "${workspaceFolder}/.vscode/compile_commands.json"
        }
    ],
    "version": 4
}
